      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Username:" />
                  <TextField promptText="username" />
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Password" />
                  <TextField promptText="password" />
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" text="Login" />
                  <Button mnemonicParsing="false" text="Create Profile" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
